# Copy this file to .env and fill in the API keys for the providers you plan to use.
# Unused providers can be left blank.

# OpenAI configuration (default provider)
OPENAI_API_KEY=
# Optional: organisation if required by your account
OPENAI_ORG=

# Gemini configuration
GOOGLE_API_KEY=

# DeepSeek configuration
DEEPSEEK_API_KEY=
# Optional: override the default API base URL
DEEPSEEK_API_BASE=https://api.deepseek.com

# Anthropic configuration
ANTHROPIC_API_KEY=

# Grok configuration
GROK_API_KEY=
# Optional: override the Grok API base URL
GROK_API_BASE=https://api.x.ai/v1
